public class Stars
{
    public static void main(String args [])
    {
        printStarTriangle(5);
    }

    public static void printStarTriangle(int x)
    {
        String j = "*";
        for (int i = 0; i < x; i++)
        {
            System.out.println(j);
            j = j + "*";
        }
        j = "*";
    }
}
